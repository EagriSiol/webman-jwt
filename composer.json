{
  "name": "tinywan/jwt",
  "description": "JSON Web Token (JWT) for webman plugin",
  "type": "library",
  "license": "MIT",
  "require": {
    "workerman/webman-framework": "^1.2.1",
    "firebase/php-jwt": "^6.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Tinywan\\Jwt\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tinywan\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "workerman/webman": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.6",
    "mockery/mockery": "^1.5"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer//"
    }
  }
}
